{
  "caliper": {
    "blockchain": "fabric"
  },
  "fabric": {
    "cryptodir": "../hlf-deployment-kafka/crypto-config",
    "network": {
      "orderer": {
        "url": "grpc://localhost:7050",
        "mspid": "OrdererMSP",
        "msp": "../hlf-deployment-kafka/crypto-config/ordererOrganizations/example.com/msp/",
        "server-hostname": "orderer0.example.com",
        "tls_cacerts": "../hlf-deployment-kafka/crypto-config/ordererOrganizations/example.com/orderers/orderer0.example.com/tls/ca.crt"
      },
      "org1": {
        "name": "peerOrg1",
        "mspid": "Org1MSP",
        "msp": "../hlf-deployment-kafka/crypto-config/peerOrganizations/org1.example.com/msp/",
        "ca": {
          "url": "http://localhost:7054",
          "name": "ca.org1.example.com"
        },
        "peer1": {
          "requests": "grpc://localhost:7051",
          "events": "grpc://localhost:7053",
          "server-hostname": "peer0.org1.example.com",
          "tls_cacerts": "../hlf-deployment-kafka/crypto-config/peerOrganizations/org1.example.com/peers/peer0.org1.example.com/tls/ca.crt"
        },
        "peer2": {
          "requests": "grpc://localhost:8051",
          "events": "grpc://localhost:8053",
          "server-hostname": "peer1.org1.example.com",
          "tls_cacerts": "../hlf-deployment-kafka/crypto-config/peerOrganizations/org1.example.com/peers/peer1.org1.example.com/tls/ca.crt"
        },
        "peer3": {
          "requests": "grpc://localhost:9051",
          "events": "grpc://localhost:9053",
          "server-hostname": "peer1.org1.example.com",
          "tls_cacerts": "../hlf-deployment-kafka/crypto-config/peerOrganizations/org1.example.com/peers/peer1.org1.example.com/tls/ca.crt"
        }
      }
    },
    "channel": [
      {
        "name": "mychannel",
        "deployed": false,
        "config": "../hlf-deployment-kafka/network-config/channel.tx",
        "organizations": ["org1"]
      }
    ],
    "chaincodes": [
      {"id": "simple", "path": "src/contract/fabric/simple/node", "language":"node", "version": "v0", "channel": "mychannel"}
    ],
    "endorsement-policy": {
      "identities": [
        {
          "role": {
            "name": "member",
            "mspId": "Org1MSP"
          }
        },
        {
          "role": {
            "name": "admin",
            "mspId": "Org1MSP"
          }
        }
      ],
      "policy": { "1-of": [{"signed-by": 0}]}
    },
    "context": {
      "open": "mychannel",
      "query": "mychannel"
    }
  },
  "info" : {
    "Version": "1.1.0",
    "Size": "1 Orgs with 3 Peers",
    "Orderer": "Kafka",
    "Distribution": "Single Host"
  }
}
